{
  "name": "nexpo-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.3.5",
    "babel-jest": "^24.9.0",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "node-sass-chokidar": "^1.5.0",
    "normalizr": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-csv": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rxjs-compat": "^6.5.5",
    "victory": "^34.3.12",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "codecov": "^3.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "flow-bin": "^0.127.0",
    "flow-typed": "^3.1.0",
    "nock": "^12.0.3",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "redux-mock-store": "^1.5.4",
    "serve": "^11.3.2"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "@testing-library/dom": "5",
    "dayjs": "^1.8.18",
    "eslint": "^7",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-react-hooks": "^4",
    "typescript": "2.8.1"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "prod-start": "REACT_APP_LOGIN=prod npm start",
    "start-js": "react-scripts start",
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "build": "npm run build-css && react-scripts build",
    "test": "npm run test-ci",
    "test-ci": "npm run build-css && CI=true npm run test-js",
    "test-js": "npm run flow && react-scripts test",
    "test-watch": "npm-run-all -p watch-css test-js",
    "test-coverage": "npm run test-js -- --coverage",
    "eject": "react-scripts eject",
    "eslint": "eslint \"src/**/*.js\"",
    "lint": "eslint . && prettier-eslint --list-different $PWD/'**/*.js'",
    "lint-fix": "prettier-eslint --write \"src/**/*.js\"",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "flow-coverage": "flow coverage",
    "format": "prettier-eslint --write $PWD/'**/*.js'",
    "postinstall": "npx flow-typed create-stub whatwg-fetch && npx flow-typed create-stub antd && npx flow-typed create-stub redux-form"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": "12.18.0",
    "npm": ">6.14.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
